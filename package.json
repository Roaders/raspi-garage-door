  "scripts": {
    "postinstall": "concurrently --kill-others-on-fail npm:install:*",
    "install:server": "cd garage-door-server && npm install",
    "install:lib": "cd rpi-garage-door && npm install",
    "clean": "rimraf dist",
    "lint": "concurrently --kill-others-on-fail npm:lint:*",
